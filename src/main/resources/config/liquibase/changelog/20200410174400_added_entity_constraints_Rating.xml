<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Rating.
    -->
    <changeSet id="20200410174400-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="review_id"
                                 baseTableName="rating"
                                 constraintName="fk_rating_review_id"
                                 referencedColumnNames="id"
                                 referencedTableName="review"/>

        <addForeignKeyConstraint baseColumnNames="film_id"
                                 baseTableName="rating"
                                 constraintName="fk_rating_film_id"
                                 referencedColumnNames="id"
                                 referencedTableName="film"/>

    </changeSet>
</databaseChangeLog>
